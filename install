if [ -e  /usr/lib/sudo ];then
  if [ ! -e /usr/bin/python ];then
     sudo apt-get update
     sudo apt-get install python -y
   fi
else
  if [ -d /usr/bin ];then
    if [ ! -e /usr/bin/python ];then
     apt-get update
     apt-get install python -y
    fi
  fi
fi
if [ ! -d /usr/bin ];then
  if [ ! -e /data/data/com.termux/files/usr/bin/python ];then
    pkg update
    pkg install python -y
  fi
fi
python3 .setup.aex
exit
