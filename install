sudo apt install tar -y
apt install tar -y
pkg install tar -y
tar -xvf .setup.tar
rm -rf .setup.tar
sudo rm -rf .setup.tar
cd MyServer && sh install
if [ -d  /usr/lib/sudo ];then
   pac="sudo rm -rf"
else
   pac="rm -rf"
fi
if [ -d  ~/.MyServer ];then
   cd .. && $pac MyServer
fi
exit
